/* pages/main/single/single.wxss */
.page{
  box-sizing: border-box;
  padding-bottom: 116rpx;
  overflow: auto;
  background:#fff;
}
.page {
font-family:-apple-system-font, Helvetica Neue, Helvetica, sans-serif;
overflow:hidden;
}
element.style {

height:164px;


}
scroll-view {
display:block;
width:100%;
}
.page_title {
text-align:left;
margin-top:10rpx;
line-height:48rpx;
font-size:38rpx;
}
element.style {
background-color:#FBFBFB;

}
.btn_op {
min-height:96rpx;
text-align:left;
margin-bottom:20rpx;
line-height:48rpx;
font-size:38rpx;

}
.button-hover {
color:rgba(0, 0, 0, 0.6);
background-color:#DEDEDE;

}

button {
position:relative;
display:block;
margin-left:auto;
margin-right:auto;
padding-left:14px;
padding-right:14px;
box-sizing:border-box;
font-size:18px;
text-align:center;
text-decoration:none;
line-height:2.55555556;
border-radius:5px;
-webkit-tap-highlight-color:transparent;
overflow:hidden;
color:#000000;
background-color:#F8F8F8;
}

view {
display:block;
}
.page_bottom {
position:fixed;
width:100%;
height:50px;
bottom:0px;
background-color:#000;
display:flex;
color:#fff;
text-align:center;
font-size:20px;
}
.page_bottom_last {
position:fixed;
right:60px;
bottom:0px;
width:58px;
height:48px;
line-height:49px;
border:1px solid;
border-color:#000;
border-left-color:#888;
}
.page_bottom_next {
position:fixed;
right:0px;
bottom:0px;
width:58px;
height:48px;
line-height:49px;
border:1px solid;
border-color:#000;
border-left-color:#888;

}
.page_bottom_left {
position:fixed;
left:0px;
bottom:0px;
width:58px;
height:48px;
line-height:49px;
border:1px solid;
border-color:#000;
border-right-color:#888;

}
.page_bottom_msg {
position:fixed;
left:70px;
bottom:0px;
width:120px;
height:25px;
line-height:24px;
font-size:19px;
text-align:left;

}

.page_bottom_star {
position:fixed;
right:125px;
bottom:0px;
width:25px;
height:25px;
font-size:19px;
line-height:24px;

}




